{
    "name": "p-nerd/quick-redirect-manager",
    "description": "A lightweight, user-friendly plugin to manage URL redirections. No database tables, just simple WordPress-native storage.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "PNerd\\QuickRedirectManager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Shihab Mahamud",
            "email": "shihab4t@gmail.com",
            "homepage": "https://github.com/p-nerd",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
